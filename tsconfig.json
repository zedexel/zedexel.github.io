{
  "compilerOptions": {
    "jsx": "react-jsx", // this is fine for React 17+
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "module": "esnext",
    "target": "es5",
    "lib": ["dom","ES2015", "esnext"]
  },
  "include": ["src"]
}
